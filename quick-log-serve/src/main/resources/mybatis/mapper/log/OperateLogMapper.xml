<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.quick.log.mapper.OperateLogMapper">

    <sql id="operateLog">
        `sys_operate_log`
    </sql>

    <sql id="Base_Column_List">
        id, operate_log_id, url, description, request_type, request_content_type, client_ip, user_agent, request_params, response_content_type, response_params, consuming_time, operating_account, create_time
    </sql>

    <insert id="insertOperateLog">
        INSERT INTO
        <include refid="operateLog"/>
        (
        <if test="id != null">
            id,
        </if>
        <if test="operateLogId != null and operateLogId != ''">
            operate_log_id,
        </if>
        <if test="url != null and url != ''">
            url,
        </if>
        <if test="description != null and description != ''">
            description,
        </if>
        <if test="requestType != null">
            request_type,
        </if>
        <if test="requestContentType != null">
            request_content_type,
        </if>
        <if test="responseContentType != null">
            response_content_type,
        </if>
        <if test="clientIp != null">
            client_ip,
        </if>
        <if test="userAgent != null">
            user_agent,
        </if>
        <if test="requestParams != null">
            request_params,
        </if>
        <if test="responseParams != null">
            response_params,
        </if>
        <if test="consumingTime != null">
            consuming_time,
        </if>
        <if test="operatingAccount != null">
            operating_account,
        </if>
        create_time
        )
        VALUES(
        <if test="id != null">
            #{id},
        </if>
        <if test="operateLogId != null and operateLogId != ''">
            #{operateLogId},
        </if>
        <if test="url != null and url != ''">
            #{url},
        </if>
        <if test="description != null and description != ''">
            #{description},
        </if>
        <if test="requestType != null">
            #{requestType},
        </if>
        <if test="requestContentType != null">
            #{requestContentType},
        </if>
        <if test="responseContentType != null">
            #{responseContentType},
        </if>
        <if test="clientIp != null">
            #{clientIp},
        </if>
        <if test="userAgent != null">
            #{userAgent},
        </if>
        <if test="requestParams != null">
            #{requestParams},
        </if>
        <if test="responseParams != null">
            #{responseParams},
        </if>
        <if test="consumingTime != null">
            #{consumingTime},
        </if>
        <if test="operatingAccount != null">
            #{operatingAccount},
        </if>
        now()
        )
    </insert>

</mapper>
